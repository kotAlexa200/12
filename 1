 using System;

public class Calculator
{
    public static double Sum(double a, double b)
    {
        return a + b;
    }

    public static double Minus(double a, double b)
    {
        return a - b;
    }

    public static double Umnojenie(double a, double b)
    {
        return a * b;
    }

    public static double Delenie(double a, double b)
    {
        if (b == 0)
        {
            Console.WriteLine("ошибка: деление на ноль");
            return double.NaN; 
        }
        else
        {
            return a / b;
        }
    }
}

public class Program
{
    static void Main()
    {
        Console.WriteLine("Сложение: " + Calculator.Sum(20, 5));
        Console.WriteLine("Вычитание: " + Calculator.Minus(20, 5));
        Console.WriteLine("Умножение: " + Calculator.Umnojenie(20, 5));
        Console.WriteLine("Деление: " + Calculator.Delenie(20, 5));
        Console.WriteLine("Деление на ноль: " + Calculator.Delenie(20, 0));

        Console.ReadLine();
    }
}
